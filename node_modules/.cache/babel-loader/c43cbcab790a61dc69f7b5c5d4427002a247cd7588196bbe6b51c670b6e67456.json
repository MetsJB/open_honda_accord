rsion":3,"names":["style","jsx","_jsx","jsxs","_jsxs","CarBody","className","container","children","src","require","background","paragraphs","list"],"sources":["C:/Users/Zahar/Desktop/Task/frontend/src/components/CarBody/CarBody.jsx"],"sourcesContent":["import style from \"./carBody.module.css\"\r\n\r\nconst CarBody = () => {\r\n  return (\r\n    <div className={style.container}>\r\n      <img\r\n        src={require(\"./../../images/кузовСтраница.png\")}\r\n        className={style.background}\r\n      />\r\n      <div className={style.paragraphs}>\r\n        <p>\r\n          По размерам новый седан Honda Accord 2018 компактнее, в сравнении с\r\n          9-ым Аккордом:\r\n        </p>\r\n      </div>\r\n      <ul className={style.list}>\r\n        <li>длина седана Honda Accord 2018 – 4882 мм;</li>\r\n        <li>ширина новинки – 1862 мм;</li>\r\n        <li>высота – 1450 мм;</li>\r\n        <li>колесная база – 2830 мм;</li>\r\n        <li>\r\n          колея передних колес – 1600 мм (для некоторых комплектаций 1590 мм);\r\n        </li>\r\n        <li>\r\n          колея задних колес – 1610 мм (для некоторых комплектаций 1603 мм).\r\n        </li>\r\n      </ul>\r\n      <div className={style.paragraphs}>\r\n        <p>\r\n          Как видим, габариты нового седана Honda Accord 2018 изменились, в\r\n          сравнении с девятым поколением. В связи с использованием новой\r\n          платформы, вес автомобиля уменьшился на 50-80 кг в зависимости от\r\n          комплектации\r\n        </p>\r\n        <p>\r\n          В новом автомобиле кузов состоит на 54% из высокопрочной стали, для\r\n          крепления крыши использовали новую технология лазерной сварки, что\r\n          позволило его слить в одно целое с основной частью кузова. За счет\r\n          доработки кузова Honda Accord 2018, центр тяжести снизился на 10 см.\r\n          Объем багажного отделения Honda Accord 2018 в стандартном варианте 473\r\n          литра, а это на 22 литра меньше, нежели в предыдущем поколении. Задние\r\n          сиденья складываются с пропорцией 60:40.\r\n        </p>\r\n      </div>\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default CarBody\r\n"],"mappings":"AAAA,MAAO,CAAAA,KAAK,KAAM,sBAAsB,QAAAC,GAAA,IAAAC,IAAA,CAAAC,IAAA,IAAAC,KAAA,yBAExC,KAAM,CAAAC,OAAO,CAAGA,CAAA,GAAM,CACpB,mBACED,KAAA,QAAKE,SAAS,CAAEN,KAAK,CAACO,SAAU,CAAAC,QAAA,eAC9BN,IAAA,QACEO,GAAG,CAAEC,OAAO,CAAC,kCAAkC,CAAE,CACjDJ,SAAS,CAAEN,KAAK,CAACW,UAAW,CAC7B,CAAC,cACFT,IAAA,QAAKI,SAAS,CAAEN,KAAK,CAACY,UAAW,CAAAJ,QAAA,cAC/BN,IAAA,MAAAM,QAAA,CAAG,mVAGH,CAAG,CAAC,CACD,CAAC,cACNJ,KAAA,OAAIE,SAAS,CAAEN,KAAK,CAACa,IAAK,CAAAL,QAAA,eACxBN,IAAA,OAAAM,QAAA,CAAI,iHAAyC,CAAI,CAAC,cAClDN,IAAA,OAAAM,QAAA,CAAI,2GAAyB,CAAI,CAAC,cAClCN,IAAA,OAAAM,QAAA,CAAI,gEAAiB,CAAI,CAAC,cAC1BN,IAAA,OAAAM,QAAA,CAAI,qGAAwB,CAAI,CAAC,cACjCN,IAAA,OAAAM,QAAA,CAAI,iTAEJ,CAAI,CAAC,cACLN,IAAA,OAAAM,QAAA,CAAI,qSAEJ,CAAI,CAAC,EACH,CAAC,cACLJ,KAAA,QAAKE,SAAS,CAAEN,KAAK,CAACY,UAAW,CAAAJ,QAAA,eAC/BN,IAAA,MAAAM,QAAA,CAAG,m9BAKH,CAAG,CAAC,cACJN,IAAA,MAAAM,QAAA,CAAG,8jEAQH,CAAG,CAAC,EACD,CAAC,EACH,CAAC,CAEV,CAAC,CAED,cAAe,CAAAH,OAAO","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}